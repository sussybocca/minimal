{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/index.js/api/chat"
    },
    {
      "src": "/api/download",
      "dest": "/api/index.js/api/download"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js/"
    }
  ]
}
